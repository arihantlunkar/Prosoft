{
    "expo": {
        "name": "Prosoft - Selection Guide",
        "slug": "ProsoftSelectionGuide",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/fakebkg.png",
            "resizeMode": "contain",
            "backgroundColor": "#02b389"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.prosoft.app"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "com.prosoft.app"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
